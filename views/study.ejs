                    <button class="tag" id="MongoDB">MongoDB</button>
                    <button class="tag" id="SpringBoot">SpringBoot</button>
                    <button class="tag" id="OracleDB">OracleDB</button>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="" style="margin-top: 2rem;"></div>
            <div class="content-all" style="flex: 11 0%;">
                <div class="card-list">
                    <% data.forEach(function (item, index){ %>
                        <% if(can[index] == 1){ %>
                            <div class="item-container" value="/detail_study/<%= item.id %>">
                                <div class="item">
                                    <div class="item-content">
                                        <div class="icon-container">
                                                <div class="icon-container">
                                                    <img class="myThumbnail" src=<%=item.icon%>>
                                                </div>
                                        </div>
                                        <div class="item-description">
                                            <div class="content-title-number">
                                                <div class="content-title"><%= item.title %></div>
                                                <div class="content-number">모집인원 <%= item.total_participant-1 %>명</div>
                                            </div>
                                            <div class="content-author">
                                                <div class="author">
                                                    <% for(var i = 0; i < authorsLevel.length;i++){ %>
                                                        <% if(item.author_id == authorsLevel[i].id){ %>
                                                            <img src="../<%= authorsLevel[i].level %>"
                                                                 style="width: 2.5rem; height: 2.5rem;">
                                                        <% } %>
                                                    <% } %>
                                                    모집자 <%=item.author_id%>
                                                </div>
                                                <div class="content-possible  blink-possible">
                                                    참가가능
                                                </div>
                                            </div>
                                            <div class="content-period">
                                                <div class="" style="display: flex;">
                                                    <div class=""
                                                         style="background-image: url('../../img/icons/calendar.svg');width: 2rem;height: 2rem;margin-right: 0.5rem; margin-left:0.4rem;">
                                                    </div>
                                                    <%
                                                        var dateString = item.date.toString();
                                                        var timeString = item.time.toString();
                                                        var date = dateString.split(',');
                                                        var time = timeString.split(',');
                                                    %>
                                                    <%
                                                    date.forEach(function(dateToken, index){ %>
                                                        <div>
                                                            <%= dateToken %>: <%= time[index] %>
                                                        </div>
                                                        <% if(date.length - 1 != index){ %>
                                                            <div>&nbsp|&nbsp</div>
                                                        <% } %>
                                                    <% }) %>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <!--                            </a>-->

                            </div>
                        <% } else{ %>
                            <div class="item-container" value="/detail_study/<%= item.id %>">
                                <div class="item-not-allowed">
                                    <div class="item-content">
                                        <div class="icon-container">
                                            <div class="icon-container">
                                                <img class="myThumbnail" src=<%=item.icon%>>
                                            </div>
                                        </div>
                                        <div class="item-description">
                                            <div class="content-title-number">
                                                <div class="content-title"><%= item.title %></div>
                                                <div class="content-number">모집인원 <%= item.total_participant-1 %>명</div>
                                            </div>
                                            <div class="content-author">
                                                <div class="author">
                                                    <% for(var i = 0; i < authorsLevel.length;i++){ %>
                                                        <% if(item.author_id == authorsLevel[i].id){ %>
                                                            <img src="../<%= authorsLevel[i].level %>"
                                                                 style="width: 2.5rem; height: 2.5rem;">
                                                        <% } %>
                                                    <% } %>
                                                  모집자 <%=item.author_id%>
                                                </div>
                                                <div class="content-impossible blink-impossible">
                                                    참가불가
                                                </div>
                                            </div>
                                            <div class="content-period">
                                                <div class="" style="display: flex;">
                                                    <div class=""
                                                         style="background-image: url('../../img/icons/calendar.svg');width: 2rem;height: 2rem;margin-right: 0.5rem; margin-left:0.4rem;">
                                                    </div>
                                                    <%
                                                        var dateString = item.date.toString();
                                                        var timeString = item.time.toString();
                                                        var date = dateString.split(',');
                                                        var time = timeString.split(',');
                                                    %>
                                                    <%
                                                    date.forEach(function(dateToken, index){ %>
                                                        <div>
                                                            <%= dateToken %>: <%= time[index] %>
                                                        </div>
                                                        <% if(date.length - 1 != index){ %>
                                                            <div>&nbsp|&nbsp</div>
                                                        <% } %>
                                                    <% }) %>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <!--                            </a>-->

                            </div>
                        <% } %>
                    <% }) %>
                    <a href="/insert_study">
                        <div class="item-container addItem" >
                            <div class="item" style="position :relative">
                                <i class="fa fa-plus addPlusBtn"></i>
                                <div class="item-content">
                                    <div class="icon-container">
                                        <div class="iconBox">
                                            
                                        </div>
                                    </div>
                                    <div class="item-description">
                                        <div class="content-title-number">
