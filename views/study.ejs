                                        </div>
                                        <div class="content-author">
                                            <div class="author content-number" style="width: 7rem; height: 2rem;">
                                            </div>
                                            <div style="margin-right: 15%;">
                                            </div>

                                            <div class="content-possible content-number"
                                                 style="width: 5rem; height: 2rem;">
                                            </div>
                                        </div>
                                        <div class="content-period">
                                            <div class="content-number"
                                                 style="display: flex; width: 5rem; height: 2rem;">
                                            </div>
                                            <div class="content-number"
                                                 style="display: flex; width: 3rem; height: 2rem;">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--                            </a>-->

                            </div>
                        </div>
                    </a>

                </div>
            </div>


        </div>


        <div class="container" style="text-align: center;">
            <ul class="pager">

                <%
                var curSet = classes.curSet
                var endPage = classes.endPage
                var startPage = classes.startPage
                var totalSet = classes.totalSet
                var totalPage = classes.totalPage
                var page_list_size = classes.page_list_size
                var page_size = classes.page_size
                var curPage = Number(classes.curPage)
                %>

                <% if(curPage == 1){ %>
                    <li value=<%=curPage%> class="previous">
                        <a href='/studies/<%=curPage%>' class="pageBtn">첫번째페이지</a>
                    </li>

                <% }else if(curPage > 1){
                %>
                    <li value=<%=curPage -1%> class="previous">
                        <a href='/studies/<%=curPage - 1%>' class="pageBtn">이전페이지</a>
                    </li>

                <%
                }
                %>

                <%
                for(var i = startPage; i <= endPage; i++){
                    if (i > totalPage) {
                        break;
                    }

                if(i == curPage){
                %>
                    <li value=<%= i %> class=''>
                        <a href="/studies/<%= i %>" class="curPageBtn">
                            <%= i %>
                        </a>
                    </li>

                <%
                }else{
                %>

                    <li value=<%= i %> class='active'>
                        <a href='/studies/<%= i %>' class="pageBtn">
                            <%= i %>
                        </a>
                    </li>

                <%
                }
                %>

                <%
                }
                %>

                <%
                if(curPage < totalPage){
                %>
                    <li value="<%curPage + 1%>" class="next">
                        <a href='/studies/<%=curPage + 1%>' class="pageBtn">다음페이지</a>
                    </li>
                <%
                }else if(curPage == totalPage){
                %>
                    <li value=<%=curPage%> class="next">
                        <a href='/studies/<%=curPage%>' class="pageBtn">마지막페이지</a>
                    </li>
                <% } %>

            </ul>

        </div>
        <!--        상세보기(모달)-->
        <div id="myModal" class="modal">
            <div id="modal-content" class="modal-content">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="fa fa-arrow-right"></i></button>
                    <iframe id="content-detail" src="" >
                </iframe>
            </div>
        </div>


    </section>

