                    <div class="partyBox">
                        <%  if(classList.length>0){%>

                        <%for(var n=0; n<classList.length;n++){%>
                            <div class="partyContent typeClass"  >
                                <div class="detailCover"  value="/detail_class/<%= classList[n].id %>"></div>
