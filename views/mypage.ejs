                                    <div class="partyCredit"><img src="../img/coin.png" style="width:24px; height:24px"> <%=classList[n].credit%></div>
                                    <div class="partyTime">
                                        <%
                                            var timeList = classList[n].time.split(",");
                                            var dateList = classList[n].date.split(",");
                                        for(var m=0;m<dateList.length;m++){%>
                                        <p><span><%=dateList[m]%>&nbsp|&nbsp<%=timeList[m]%></span> </p>
                                        <%}
                                            console.log(position_id_class[n]);

                                        %>
                                    </div>


                                    <input class="posNum" style="display:none" value=<%=position_id_class[n]%> >
                                    <button class="cancelBtn classCancelBtn" value =<%=classList[n].id %> >취소</button>
                                </div>
                            </div>
                        <%}%>

                        <% }else{ %>
